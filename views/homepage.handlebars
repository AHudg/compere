<section>
  <div class="row justify-content-center text-center quiz-section">
    <h2 class="col-5 col-md-2 text-end">Search By Quiz:</h2>
    <input class="col-6 col-md-5" type="searchQuery" id="searchQuery" />
  </div>

  <h3>Our Current Quizzes</h3>

  {{#each quizzes as |quiz| }} 
    <div class="row justify-content-around text-center quiz-section">
      {{> populate-quiz quiz}}
    </div>
  {{/each}}
</section>