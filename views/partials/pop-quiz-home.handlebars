<div class="col-10 col-md-5 quiz-body">
  <div class="quiz-like">14k likes
    <i id="{{id}}" class="fa-regular fa-heart"></i></div>
  <img class="quiz-img" src={{img_url}} />
  <div class="quiz-content">
    <h4 id="quiz-title">{{title}}</h4>
    <p>
      {{description}}
    </p>
    <a href="/quiz/{{{id}}}" class="view-btn">More Quiz Details</a> 
    <br /><br />
  </div>
</div>